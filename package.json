{
  "name": "brx-node",
  "version": "0.7.3-development",
  "description": "Main BRX-AI Node interface",
  "type": "module",
  "main": "lib/esm/src/index.js",
  "types": "lib/esm/src/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json && node ./CJSPostBuild.js",
    "build:watch": "tsc --watch --project ./tsconfig.esm.json",
    "clean": "rm -rf ./lib/",
    "cm": "cz",
    "lint": "eslint ./src/ --fix",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "node --env-file=./.env ./lib/esm/test/esm/brk_run.js",
    "test:create": "node --env-file=./.env ./lib/test/fast_create_next.js",
    "only_test": "jest",
    "typecheck": "tsc --noEmit",
    "release": "npm publish",
    "api-test": "ts-node ./test/api_interact"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brx-ai/brx-node.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jando",
    "email": "Brx-ai@users.noreply.github.com",
    "url": "https://github.com/Brx-ai"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "brx",
    "brx-node",
    "ai",
    "template",
    "typescript",
    "vscode",
    "jest"
  ],
  "exports": {
    ".": {
      "require": "./lib/cjs/src/index.cjs",
      "import": "./lib/esm/src/index.js",
      "types": "./lib/esm/src/*.d.ts",
      "default": "./lib/esm/src/*.js"
    }
  },
  "bugs": {
    "url": "https://github.com/Brx-ai/brx-node/issues"
  },
  "homepage": "https://github.com/Brx-ai/brx-node#readme",
  "devDependencies": {
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "semantic-release": "^23.0.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "dependencies": {
    "@types/ws": "^8.5.10",
    "axios": "^1.7.2",
    "eslint-plugin-import": "^2.29.1",
    "ws": "^8.16.0"
  }
}